{
  "name": "bec-lang",
  "displayName": "BEc-lang",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "colors": [
      {
        "id": "keyword.flow.bec",
        "description": "Color for keywords in Your Language",
        "defaults": {
          "dark": "#C586C0",
          "light": "#C586C0"
        }
      }
    ],
    "languages": [
      {
        "id": "bec",
        "aliases": [
          "BEc_lang",
          "bec"
        ],
        "extensions": [
          ".bec"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "BEC.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "BEC.TEST",
        "title": "TEST"
      }
    ],
    "grammars": [
      {
        "language": "bec",
        "scopeName": "source.bec",
        "path": "./syntaxes/bec.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.0.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.39",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^4.3.5",
    "@vscode/test-electron": "^2.3.4",
    "vscode-test": "^1.5.1"
  }
}