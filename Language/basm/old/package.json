{
    "name": "basm",
    "displayName": "Basm",
    "description": "",
    "version": "0.0.1",
    "engines": {
      "vscode": "^1.86.0"
    },
    "categories": [
      "Programming Languages"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
      "languages": [
        {
          "id": "BE assembly",
          "aliases": ["BE assembly", "BEass"],
          "extensions": [".basm"],
          "configuration": "./language-configuration.json"
        }
      ],
      "commands": [{
        "command": "basm.helloWorld",
        "title": "Hello World"
      }],
      "grammars": [{
        "language": "BE assembly",
        "scopeName": "source.basm",
        "path": "./syntaxes/basm.tmLanguage.json"
      }]
    },
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc",
      "watch": "tsc -w",
      "test": "node ./out/test/runTest.js"
    },
    "keywords": [
      "vscode"
    ],
    "dependencies": {
      "vscode": "^1.2.3"
    },
    "devDependencies": {
      "@types/vscode": "^1.0.0",
      "@types/mocha": "^8.2.2",
      "@types/node": "^14.14.39",
      "eslint": "^8.47.0",
      "glob": "^10.3.3",
      "mocha": "^10.2.0",
      "typescript": "^4.3.5",
      "@vscode/test-electron": "^2.3.4",
      "vscode-test": "^1.5.1"
    }
  }